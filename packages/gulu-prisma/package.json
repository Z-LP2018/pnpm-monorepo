      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "devDependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/core": "^11.0.1",
    "@types/node": "^20.0.0",
    "prisma": "^7.0.1",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "@prisma/client": "^7.0.1"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "typescript",
    "types",
    "monorepo",
    "shared"
  ]
