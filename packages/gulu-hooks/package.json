  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@gulu/types": "workspace:*",
    "@types/node": "^24.10.0",
    "rimraf": "^6.0.1",
    "typescript": "~5.9.3",
    "tsup": "^8.5.1"
  }
}
